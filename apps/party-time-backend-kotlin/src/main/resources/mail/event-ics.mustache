BEGIN:VCALENDAR
    VERSION:2.0
    PRODID:{{website}}
    CALSCALE:GREGORIAN
    BEGIN:VTIMEZONE
        TZID:Europe/Berlin
        TZURL:http://tzurl.org/zoneinfo-outlook/Europe/Berlin
        X-LIC-LOCATION:Europe/Berlin
        BEGIN:DAYLIGHT
            TZNAME:CEST
            TZOFFSETFROM:+0100
            TZOFFSETTO:+0200
            DTSTART:19700329T020000
            RRULE:FREQ=YEARLY;BYMONTH=3;BYDAY=-1SU
        END:DAYLIGHT
        BEGIN:STANDARD
            TZNAME:CET
            TZOFFSETFROM:+0200
            TZOFFSETTO:+0100
            DTSTART:19701025T030000
            RRULE:FREQ=YEARLY;BYMONTH=10;BYDAY=-1SU
        END:STANDARD
    END:VTIMEZONE
    BEGIN:VEVENT
        DTSTAMP:{{generatedTimestamp}}
        ORGANIZER;CN="{{event.organizerName}}":MAILTO:{{organizerEmail}}
        UID:{{uid}}
        DTSTART;TZID=Europe/Berlin:{{start}}
        DTEND;TZID=Europe/Berlin:20130710T190000
        SUMMARY:{{event.eventName}}
        LOCATION:{{event.location}}
    END:VEVENT
END:VCALENDAR
